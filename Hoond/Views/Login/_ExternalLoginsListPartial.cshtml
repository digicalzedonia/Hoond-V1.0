@model ICollection<AuthenticationClientData>
@using Hoond.app_GlobalResources;
@if (Model.Count == 0)
{
    <div class="message-info">
        <p>@Content.titulo_ExternalLogin</p>
    </div>
}
else
{
    using (Html.BeginForm("ExternalLogin", "Login", new { ReturnUrl = ViewBag.ReturnUrl }))
    {
    @Html.AntiForgeryToken()
    <fieldset id="socialLoginList">
        <legend>@Content.msg_ExternalLogin</legend>
        <p>
        @foreach (AuthenticationClientData p in Model)
        {
            <button type="submit" name="provider" value="@p.AuthenticationClient.ProviderName">@p.DisplayName</button>
        }
        </p>
    </fieldset>
    }
}
